{
    "containerDefinitions": [
        {
            "name": "<image-name>",
            "image": "730335463484.dkr.ecr.us-east-1.amazonaws.com/<image-name>",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "<image-name>-8000-tcp",
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/<image-name>",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::730335463484:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::730335463484:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "family": "<image-name>",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false,
    "tags": []
}